{
  "expo": {
    "name": "apptemplate",
    "slug": "apptemplate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "apptemplate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vuongdovu1.apptemplate",
      // "usesAppleSignIn": false,
      "appleTeamId": "Y53WF4HJUU",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.804136644010-dlh0rh7ut1ru7kmea8me62crbldnsd6b",
              "app-1-804136644010-ios-601ea32b3498f272cdcfd3"
            ]
          }
        ],
        "NSCameraUsageDescription": "This app uses the camera to take photos.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to record audio.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show nearby places on the map."
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.vuongdovu1.apptemplate",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      // [
      //   "expo-apple-authentication"
      // ],
      [
        "@react-native-google-signin/google-signin"
      ],
      "./plugins/with-rnfirebase-non-modular-headers",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-maps",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
